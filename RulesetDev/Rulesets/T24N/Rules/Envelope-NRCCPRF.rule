  ANNUAL_PROPOSED
    nrccTableFins:Name
ENDRULE

RULE nrcctblRowFins:Fins01_FenestrationName
  ANNUAL_PROPOSED
    if (LocalCompAssigned(noXMLoutput_WinRef)) then noXMLoutput_WinRef:Name
    else if (LocalCompAssigned(noXMLoutput_ResWinRef)) then noXMLoutput_ResWinRef:Name
    else UNDEFINED
    endif endif 
ENDRULE
RULE nrcctblRowFins:Fins0203Subheading
  ANNUAL_PROPOSED
    if (LocalCompAssigned(noXMLoutput_WinRef)) then if (IfValidAnd(noXMLoutput_WinRef:LeftFinDepth >= 0.05)) then "Left Fin" else UNDEFINED endif
    else if (LocalCompAssigned(noXMLoutput_ResWinRef)) then if (IfValidAnd(noXMLoutput_ResWinRef:LeftFinDepth >= 0.05)) then "Left Fin" else UNDEFINED endif
    else UNDEFINED
    endif endif 
ENDRULE
RULE nrcctblRowFins:Fins02_FinDepth
  ANNUAL_PROPOSED
    if (LocalCompAssigned(noXMLoutput_WinRef)) then atof( Format( "%.2f", noXMLoutput_WinRef:LeftFinDepth ) )
    else if (LocalCompAssigned(noXMLoutput_ResWinRef)) then atof( Format( "%.2f", noXMLoutput_ResWinRef:LeftFinDepth ) )
    else UNDEFINED
    endif endif 
ENDRULE
RULE nrcctblRowFins:Fins03_FinDistanceLeft
  ANNUAL_PROPOSED
    if (LocalCompAssigned(noXMLoutput_WinRef)) then atof( Format( "%.2f", noXMLoutput_WinRef:LeftFinDistance ) )
    else if (LocalCompAssigned(noXMLoutput_ResWinRef)) then atof( Format( "%.2f", noXMLoutput_ResWinRef:LeftFinDistL ) )
    else UNDEFINED
    endif endif 
ENDRULE
RULE nrcctblRowFins:Fins0405Subheading
  ANNUAL_PROPOSED
    if (LocalCompAssigned(noXMLoutput_WinRef)) then if (IfValidAnd(noXMLoutput_WinRef:RightFinDepth >= 0.05)) then "Right Fin" else UNDEFINED endif
