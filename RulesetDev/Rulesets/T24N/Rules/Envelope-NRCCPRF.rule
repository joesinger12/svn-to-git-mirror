         then atof( Format( "%.2f", noXMLoutput_ResCathedralCeilingRef:RoofRise ) )
         else UNDEFINED
         endif
    else if (LocalCompAssigned(noXMLoutput_ResAtticRef)) 
         then if (IfValidAnd(noXMLoutput_ResAtticRef:RoofRise >= 0)) 
              then atof( Format( "%.2f", noXMLoutput_ResAtticRef:RoofRise ) )
              else UNDEFINED
              endif
