RULE nrcctblRowOpaq:Opaq01_ConstructionAssemblyNameString
  ANNUAL_PROPOSED
    if (LocalCompAssigned(noXMLoutput_ConsAssmRef)) then noXMLoutput_ConsAssmRef:Name
    else if (LocalCompAssigned(noXMLoutput_ResConsAssmRef)) then noXMLoutput_ResConsAssmRef:Name
    else UNDEFINED
    endif endif
ENDRULE
RULE nrcctblRowOpaq:Opaq02_CompatibleSurfaceType
  ANNUAL_PROPOSED
    if (LocalCompAssigned(noXMLoutput_ConsAssmRef)) 
    then noXMLoutput_ConsAssmRef:CompatibleSurfType
    else UNDEFINED
    endif
ENDRULE
RULE nrcctblRowOpaq:Opaq02_OpaqueSurfaceType
  ANNUAL_PROPOSED
    if (LocalCompAssigned(noXMLoutput_ConsAssmRef)) 
    then UNDEFINED
    else if (LocalCompAssigned(noXMLoutput_ResConsAssmRef))
         then switch (noXMLoutput_ResConsAssmRef:CanAssignTo)
                    case "Exterior Walls"         : "ExteriorWall"
                    case "Interior Walls"         : "InteriorWall"
                    case "Underground Walls"      : "UndergroundWall"
                    case "Attic Roofs"            : "AtticRoof"
                    case "Cathedral Ceilings"     : "CeilingCathedral"
                    case "Ceilings (below attic)" : "CeilingBelowAttic"
                    case "Interior Ceilings"      : "CeilingInterior"
                    case "Interior Floors"        : "InteriorFloor"
                    case "Exterior Floors"        : "ExteriorFloor"
                    case "Floors Over Crawlspace" : "FloorOverCrawlspace"
                    default :  UNDEFINED
              endswitch
         else UNDEFINED
         endif
    endif
ENDRULE
RULE nrcctblRowOpaq:Opaq03_SurfaceArea
  ANNUAL_PROPOSED
    if (LocalCompAssigned(noXMLoutput_ConsAssmRef)) 
    then switch (noXMLoutput_ConsAssmRef:Status)
            case "New"      : atof( Format( "%.2f", noXMLoutput_ConsAssmRef:AreaNew ) )
            case "Existing" : atof( Format( "%.2f", noXMLoutput_ConsAssmRef:AreaExisting ) )
            case "Altered"  : atof( Format( "%.2f", noXMLoutput_ConsAssmRef:AreaAltered ) )
            default :  UNDEFINED
         endswitch
    else if (LocalCompAssigned(noXMLoutput_ResConsAssmRef))
         then atof( Format( "%.2f", noXMLoutput_ResConsAssmRef:AssignedSurfaceArea ) )
         else UNDEFINED
         endif
    endif
ENDRULE
RULE nrcctblRowOpaq:Opaq04_PartitionFramingMaterial
  ANNUAL_PROPOSED
    if (LocalCompAssigned(noXMLoutput_ConsAssmRef)) 
    then switch (noXMLoutput_ConsAssmRef:FrmMat)
            case "Wood"  : "WoodFraming"
            case "Metal" : "Metal"
            default :  UNDEFINED
         endswitch
    else UNDEFINED
    endif
ENDRULE
RULE nrcctblRowOpaq:Opaq04_ConstructionAssemblyType
  ANNUAL_PROPOSED
    if (LocalCompAssigned(noXMLoutput_ConsAssmRef)) 
    then UNDEFINED
    else if (LocalCompAssigned(noXMLoutput_ResConsAssmRef))
         then switch (noXMLoutput_ResConsAssmRef:Type)
                  case "Wood Framed Wall"         :  "WoodFramedWall"
                  case "Steel Framed Wall"        :  "SteelFramedWall"
                  case "Concrete / ICF / Brick"   :  "ConcreteBrickICF"
                  case "Hollow Unit Masonry"      :  "HollowUnitMasonry"
                  case "Adobe / Strawbale / Log"  :  "AdobeStrawbaleLog"
                  case "SIPS Wall"                :  "SIPS_Wall"
                  case "Dual Panel Hollow (DPH) Wall" : "DualPanelHollowWall"    ; SAC 12/31/22
                  case "No Frame (concrete/CMU)"  :  "NoFrameConcreteCMU"
                  case "Wood Framed Ceiling"      :  "WoodFramedCeiling"
                  case "Built-up Roof"            :  "BuiltupRoof"
                  case "SIPS Ceiling"             :  "SIPS_Ceiling"
                  case "Wood Framed Floor"        :  "WoodFramedFloor"
                  case "SIPS Floor"               :  "SIPS_Floor"
                  default  :  UNDEFINED
              endswitch
         else UNDEFINED
         endif
    endif
ENDRULE
RULE nrcctblRowOpaq:Opaq04_NotApplicableMessage
  ANNUAL_PROPOSED
    if (LocalStatus( Opaq04_PartitionFramingMaterial ) > 0 .OR. LocalStatus( Opaq04_ConstructionAssemblyType ) > 0)
    then  UNDEFINED  else  "NotApplicable"  endif
ENDRULE
RULE nrcctblRowOpaq:Opaq05_CavityInsulation
  ANNUAL_PROPOSED
    if (LocalCompAssigned(noXMLoutput_ConsAssmRef)) 
    then if (IfValidAnd(noXMLoutput_ConsAssmRef:CavityInsVal >= 0)) then atof( Format( "%.2f", noXMLoutput_ConsAssmRef:CavityInsVal ) ) else UNDEFINED endif
    else if (LocalCompAssigned(noXMLoutput_ResConsAssmRef))
         then if (IfValidAnd(noXMLoutput_ResConsAssmRef:CavityLayerRVal >= 0)) then atof( Format( "%.2f", noXMLoutput_ResConsAssmRef:CavityLayerRVal ) ) else UNDEFINED endif
         else UNDEFINED
         endif
    endif
ENDRULE
RULE nrcctblRowOpaq:Opaq05_NotApplicableMessage
  ANNUAL_PROPOSED
    if (LocalStatus( Opaq05_CavityInsulation ) > 0)
    then  UNDEFINED  else  "NotApplicable"  endif
ENDRULE

RULE nrcctblRowOpaq:Opaq06int_InsulationRValue
  ANNUAL_PROPOSED
    if (LocalCompAssigned(noXMLoutput_ConsAssmRef)) 
    then UNDEFINED
    else if (LocalCompAssigned(noXMLoutput_ResConsAssmRef))
        then if (EnumValue(noXMLoutput_ResConsAssmRef:CanAssignTo) = 0 .OR. EnumValue(noXMLoutput_ResConsAssmRef:CanAssignTo) = 10 .OR. EnumValue(noXMLoutput_ResConsAssmRef:CanAssignTo) = 20)            ; Exterior Walls / Interior Walls / Underground Walls
             then  if (EnumValue(noXMLoutput_ResConsAssmRef:Type) < 2 .OR. EnumValue(noXMLoutput_ResConsAssmRef:Type) = 5)   ; Wood Framed Wall / Steel Framed Wall / SIPS Wall
                   then  if (EnumValue(noXMLoutput_ResConsAssmRef:SheathInsul2Layer) < 99000 .OR. EnumValue(noXMLoutput_ResConsAssmRef:SheathInsul2Layer) > 100000)  ; none, Gypsum or Wood
                         then  0
                         else if (EnumValue(noXMLoutput_ResConsAssmRef:SheathInsul2Layer) = 99000)
                              then  ValidOr( noXMLoutput_ResConsAssmRef:SheathInsul2LayerRVal, 0 )
                              else  EnumValue(noXMLoutput_ResConsAssmRef:SheathInsul2Layer) - 99000
                              endif
                         endif
                   else                          ; Concrete / ICF / Brick -/- Hollow Unit Masonry -/- Adobe / Strawbale / Log
                         if (EnumValue(noXMLoutput_ResConsAssmRef:FurringInsul2Layer) = 100401)     ; none or furring present
                         then  0
                         else if (EnumValue(noXMLoutput_ResConsAssmRef:FurringInsul2Layer) = 900)
                              then  ValidOr( noXMLoutput_ResConsAssmRef:FurringInsul2LayerRVal, 0 )
                              else  EnumValue(noXMLoutput_ResConsAssmRef:FurringInsul2Layer)
                              endif
                         endif 
                   endif
             else if (EnumValue(noXMLoutput_ResConsAssmRef:CanAssignTo) = 1)                          ; Attic Roofs  
                  then  0     ; only Exterior continuous
