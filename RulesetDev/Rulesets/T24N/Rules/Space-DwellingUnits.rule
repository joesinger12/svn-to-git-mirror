    else  "Electric"
    endif
ENDRULE

// -----------------------------------------------------------------------------
RULE Spc:DwellingUnitClothesWasher
  DESCRIPTION
    "whether or not dwelling units of this space include clothes washers"  
  DEFAULT
    if (IsRes == 0) then  UNDEFINED
    else if (SpcFunc = "High-Rise Residential Living Spaces")
    then  1
    else  0
    endif endif
ENDRULE

