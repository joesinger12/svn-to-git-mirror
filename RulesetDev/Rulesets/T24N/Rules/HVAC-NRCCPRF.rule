    else if (LocalCompAssigned(noXMLoutput_ZnSysRef)) then if (noXMLoutput_ZnSysRef:OperableWinInterlockRpt != "NA") 
                                                           then 
                                                               switch (noXMLoutput_ZnSysRef:OperableWinInterlockRpt)
