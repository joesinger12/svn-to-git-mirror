    else if (LocalCompAssigned(noXMLoutput_VRFSysRef)) then if (noXMLoutput_VRFSysRef:OperableWinInterlockRpt != "NA") 
                                                            then 
                                                                switch (noXMLoutput_VRFSysRef:OperableWinInterlockRpt)
                                                                    case "Yes" : "true"
                                                                    case "No"  : "false"
                                                                    default :  UNDEFINED
                                                                endswitch
                                                            else UNDEFINED endif
    else UNDEFINED
    endif endif endif
