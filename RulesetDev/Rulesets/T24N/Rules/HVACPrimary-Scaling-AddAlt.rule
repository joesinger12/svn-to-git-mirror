// -----------------------------------------------------------------------------
// Define tolerances for scaling
RULE NEW Proj:ClgScalingTol
  DATATYPE
    FLoat
  LONGFORM
    CoolingScalingTolerance
  DESCRIPTION
    "The tolerance on plant cooling capacity before scaling is applied."
  INPUTCLASS 
    NotInput
  DEFAULT 
    0.9
ENDRULE

RULE NEW Proj:HtgScalingTol
  DATATYPE
    FLoat
  LONGFORM
    HeatingScalingTolerance
  DESCRIPTION
    "The tolerance on plant heating capacity before scaling is applied."
  INPUTCLASS 
    NotInput
  DEFAULT 
    0.9
ENDRULE

// -----------------------------------------------------------------------------
// Set a flag if scaling is required and calculate scalars
RULE NEW FluidSys:ScalingReq
  DATATYPE
    Integer
  LONGFORM
    ScalingRequired
  INPUTCLASS
    NotInput
  DESCRIPTION
    "A flag that indicates FluidSys equipment capacities are scaled (down) in the compliance 
     analysis."
  HELP
    "Scaling is supported for two scenarios:
     
     1) An Addition, Alteration, or AdditionOrAlteration model where the whole building
     is not modeled and the proposed plant is Existing. The model may or may not 
     include Healthcare systems.

     2) A CoreAndShell model where the proposed plant is sized to serve the New 
     core systems as well has capacity to serve future systems. The model may or 
     may not include Healthcare systems.

     Currently, only down-scaling is supported, i.e. it is assumed that the proposed
     plant capacity is greater than what is needed to serve the systems that are
     included in the model."
  DEFAULT
    if( IsAddOrAlt > 0 .AND.
        IfValidAnd( Bldg:WholeBldgModeled = 0 ) .AND. 
        IfValidAnd( Bldg:WholePlantModeled > 0 ) )
    then // Is an Add/Alt model where the whole building is not modeled but
         // the whole plant is -or- has Healthcare
      if( IsNew > 0 )
      then // Is a New plant
        if( Type = "ChilledWater" .AND.
            CoilClgCapHydronicNew < ValidOr( ClgCap, 0 ) * Proj:ClgScalingTol )
        then 1
        else
        if( Type = "HotWater" .AND.
            CoilHtgCapHydronicNew < ValidOr( HtgCap, 0 ) * Proj:HtgScalingTol )
        then 1
        else
        if( IsWSHPSys > 0 .AND.
            ( CoilClgCapWSHPNew < ValidOr( ClgCap, 0 ) * Proj:ClgSizingRat .OR.
              CoilHtgCapWSHPNew < ValidOr( HtgCap, 0 ) * Proj:HtgScalingTol ) )
        then 1
        else 
        if( Type = "CondenserWater" .AND. 
            SumRevRef( FluidSys:CWSysRef, FluidSys:ScalingReq ) > 0 )
        then MaxRevRef( FluidSys:CWSysRef, FluidSys:ScalingReq )
        else 0
        endif endif endif endif
      else // Is an Existing plant
      if( Type = "ChilledWater" .AND.
          CoilClgCapHydronicExisting < ValidOr( ClgCap, 0 ) * Proj:ClgScalingTol )
      then 1
      else
      if( Type = "HotWater" .AND.
          CoilHtgCapHydronicExisting < ValidOr( HtgCap, 0 ) * Proj:HtgScalingTol )
      then 1
      else
      if( IsWSHPSys > 0 .AND.
          ( CoilClgCapWSHPExisting < ValidOr( ClgCap, 0 ) * Proj:ClgSizingRat .OR.
            CoilHtgCapWSHPExisting < ValidOr( HtgCap, 0 ) * Proj:HtgScalingTol ) )
      then 1
      else 
      if( Type = "CondenserWater" .AND. 
          SumRevRef( FluidSys:CWSysRef, FluidSys:ScalingReq ) > 0 )
      then MaxRevRef( FluidSys:CWSysRef, FluidSys:ScalingReq )
      else 0
      endif endif endif endif endif
    else
    if( Proj:IsPartMech > 0 .AND. Proj:PropHasUnknownHVACZns > 0 )
    then // Is a Core and Shell model
      if( Type = "ChilledWater" .AND. 
          CoilClgCapHydronicNew < ValidOr( ClgCap, 0 ) * Proj:ClgScalingTol )
      then 2
      else
      if( Type = "HotWater" .AND.
          CoilHtgCapHydronicNew < ValidOr( HtgCap, 0 ) * Proj:HtgScalingTol )
      then 2
      else
      if( IsWSHPSys > 0 .AND.
          ( CoilClgCapWSHPNew < ValidOr( ClgCap, 0 ) * Proj:ClgSizingRat .OR.
            CoilHtgCapWSHPNew < ValidOr( HtgCap, 0 ) * Proj:HtgScalingTol ) )
      then 2
      else 
      if( Type = "CondenserWater" .AND. 
          SumRevRef( FluidSys:CWSysRef, FluidSys:ScalingReq ) > 0 )
      then MaxRevRef( FluidSys:CWSysRef, FluidSys:ScalingReq )
      else 0
      endif endif endif endif
    else 0
    endif endif
  CHECKSIM
    if( ResExistingFlrAreaServed > 0 .AND.
        ( CoilClgCapHydronicExisting > 0 .OR.
          CoilHtgCapHydronicExisting > 0 .OR.
          CoilClgCapWSHPExisting > 0 .OR.
          CoilHtgCapWSHPExisting > 0 ) )
    then PostError( "FluidSystem '%s' serves coils in existing residential and non-residential
                     systems, which is not currently supported. Split the system up to serve
                     residential and non-residential separately (split capacity according to
                     proportion served, and keep efficiency the same for both FluidSystems)."
                     , Name )
    else
    if( ResExistingFlrAreaServed > 0 .AND.
        ResExistingFlrAreaServed < 0.8 * ResFlrAreaServed )
    then PostError( "FluidSystem '%s' serves coils in both new and existing residential systems, 
                     and the floor area served by the existing systems is less than 80% of the 
                     total area served by this FluidSystem, which is not currently supported. 
                     Split the system up to serve new and existing systems separately (split 
                     capacity according to proportion served, and keep efficiency the same for 
                     both FluidSystems).", Name )
    else UNCHANGED
    endif endif
  SIZING_PROPOSED
    if( IsBaseSys > 0 )
    then 0
    else ScalingReq
    endif
  SIZING_BASELINE
    if( IsBaseSys > 0 )
    then 0
    else ScalingReq
    endif
; PREANALYSISCHECK
;   if( ScalingReq > 0 )
;   then
;     PostWarning("FluidSystem '%s' serves coils in both new and existing
;                  system(s). Existing systems will be passed through to the baseline
;                  along with this FluidSystem, however, the proposed primary equipment
;                  will not be scaled down to the capacity needed to only serve the
;                  existing system(s). This may result in incorrect compliance 
;                  analysis results.", Name)
;   else UNCHANGED
;   endif
ENDRULE
RULE NEW FluidSys:ScalingForHlthCareReq
  DATATYPE
    Integer
  LONGFORM
    ScalingForHealthCareRequired
  INPUTCLASS
    NotInput
  DESCRIPTION
    "A flag that indicates FluidSys equipment capacities are scaled (down) in the compliance 
     analysis for healthcare in standard design."
  DEFAULT
    if( IsExisting = 0 .AND. (
        CoilClgCapHydronicHlthCare > 0 .OR.
        CoilHtgCapHydronicHlthCare > 0 .OR.
        CoilClgCapWSHPHlthCare > 0 .OR.
        CoilHtgCapWSHPHlthCare > 0 ) )
    then
      if( Type = "ChilledWater" .AND.
          CoilClgCapHydronicHlthCare < ValidOr( ClgCap, 0 ) * Proj:ClgScalingTol )
      then 1
      else
      if( Type = "HotWater" .AND.
          CoilHtgCapHydronicHlthCare < ValidOr( HtgCap, 0 ) * Proj:HtgScalingTol )
      then 1
      else
      if( IsWSHPSys > 0 .AND.
          ( CoilClgCapWSHPHlthCare < ValidOr( ClgCap, 0 ) * Proj:ClgScalingTol .OR.
            CoilHtgCapWSHPHlthCare < ValidOr( HtgCap, 0 ) * Proj:HtgScalingTol ) )
      then 1
      else 0
      endif endif endif
    else if( Type = "CondenserWater" .AND. 
             SumRevRef( FluidSys:CWSysRef, FluidSys:ScalingForHlthCareReq ) > 0 )
    then 1
    else 0
    endif endif
  ANNUAL
    if( IsBaseHlthCareSys > 0 .AND. IsExisting = 0 )
    then ScalingForHlthCareReq
    else 0
    endif
ENDRULE

RULE NEW FluidSys:ClgScalar
  DATATYPE
    Float
  LONGFORM
    CoolingScalar
  DESCRIPTION
    "The scalar by which FluidSys cooling equipment is scaled."
  INPUTCLASS
    NotInput
  DEFAULT
    if( ScalingReq > 1 )
    then // Scaling is required for Core and Shell
      if( Type = "ChilledWater" .AND. IfValidAnd( ClgCap > 0 ) ) 
      then
        Min( CoilClgCapHydronicNew / ClgCap, 1 )
      else
      if( IsWSHPSys > 0 .AND. IfValidAnd( ClgCap > 0 ) )
      then // Calculate scalar for WSHP cooling (heat rejection)
        Min( CoilClgCapWSHPNew / ClgCap, 1 )
      else
      if( Type = "CondenserWater" .AND. IfValidAnd( ClgCap > 0 ) )
      then // Set CW system scalar to the same as the ChW system it serves
        MaxRevRef( FluidSys:CWSysRef, FluidSys:ClgScalar )
      else 1 // No scaling
      endif endif endif
    else
    if( ScalingReq > 0 )
    then // Scaling required for Add/Alt
      if( IsNew > 0 )
      then // Is a New plant
        if( Type = "ChilledWater" .AND. IfValidAnd( ClgCap > 0 ) ) 
        then
          Min( CoilClgCapHydronicNew / ValidOr( Bldg:CoilClgCapHydronic, ClgCap ), 1 )
        else
        if( IsWSHPSys > 0 .AND. IfValidAnd( ClgCap > 0 ) )
        then // Calculate scalar for WSHP cooling (heat rejection)
          Min( CoilClgCapWSHPNew / ClgCap, 1 )
        else
        if( Type = "CondenserWater" .AND. IfValidAnd( ClgCap > 0 ) )
        then // Set CW system scalar to the same as the ChW system it serves
          MaxRevRef( FluidSys:CWSysRef, FluidSys:ClgScalar )
        else 1 // No scaling
        endif endif endif
      else // Is an Existing plant
      if( Type = "ChilledWater" .AND. IfValidAnd( ClgCap > 0 ) ) 
      then
        Min( CoilClgCapHydronicExisting / ValidOr( Bldg:CoilClgCapHydronic, ClgCap ), 1 )
      else
      if( IsWSHPSys > 0 .AND. IfValidAnd( ClgCap > 0 ) )
      then // Calculate scalar for WSHP cooling (heat rejection)
        Min( CoilClgCapWSHPExisting / ClgCap, 1 )
      else
      if( Type = "CondenserWater" .AND. IfValidAnd( ClgCap > 0 ) )
      then // Set CW system scalar to the same as the ChW system it serves
        MaxRevRef( FluidSys:CWSysRef, FluidSys:ClgScalar )
      else 1 // No scaling
      endif endif endif endif
    else 1
    endif endif
;  CHECKSIM
;    if( ScalingReq > 0 .AND. ClgScalar > 1.0 )
;    then
;      PostError("Plant scaling is required for FluidSystem '%s', but the 
;                 cooling scalar calculated is greater than 1. This indicates
;                 the secondary (air-side) equipment capacity is much greater
;                 than the primary (water-side) equipment capacity. 
;                 Check inputs for consistency.",Name)
;   else UNCHANGED
;   endif
ENDRULE
RULE NEW FluidSys:ClgHlthCareScalar
  DATATYPE
    Float
  LONGFORM
    CoolingHealthCareScalar
  DESCRIPTION
    "The scalar by which basline Healthcare FluidSys cooling equipment is scaled."
  INPUTCLASS
    NotInput
  DEFAULT
    if( ScalingForHlthCareReq > 0 )
    then
      if( Type = "ChilledWater" .AND. IfValidAnd( ClgCap > 0 ) ) 
      then
        Min( CoilClgCapHydronicHlthCare / ClgCap, 1 )
      else
      if( IsWSHPSys > 0 .AND. IfValidAnd( ClgCap > 0 ) )
      then // Calculate scalar for WSHP cooling (heat rejection)
        Min( CoilClgCapWSHPHlthCare / ClgCap, 1 )
      else
      if( Type = "CondenserWater" .AND. IfValidAnd( ClgCap > 0 ) )
      then // Set CW system scalar to the same as the ChW system it serves
        MaxRevRef( FluidSys:CWSysRef, FluidSys:ClgHlthCareScalar )
      else 1 // No scaling
      endif endif endif
    else 1
    endif
ENDRULE

RULE NEW FluidSys:HtgScalar
  DATATYPE
    Float
  LONGFORM
    HeatingScalar
  DESCRIPTION
    "The scalar by which FluidSys heating equipment is scaled."
  INPUTCLASS
    NotInput
  DEFAULT
    if( IfValidAnd( ScalingReq > 1 ) )
    then // Scaling is required for Core and Shell
      if( Type = "HotWater" .AND. IfValidAnd( HtgCap > 0 ) )
      then
        Min( CoilHtgCapHydronicNew / HtgCap, 1 )
      else
      if( IsWSHPSys > 0 .AND. IfValidAnd( HtgCap > 0 ) )
      then // Calculate scalar for WSHP cooling (heat rejection)
        Min( CoilHtgCapWSHPNew / HtgCap, 1 )
      else 1 // No scaling
      endif endif
    else
    if( IfValidAnd( ScalingReq > 0 ) )
    then // Scaling required for Add/Alt
      if( IsNew > 0 )
      then
        if( Type = "HotWater" .AND. IfValidAnd( HtgCap > 0 ) )
        then
          Min( CoilHtgCapHydronicNew / ValidOr( Bldg:CoilHtgCapHydronic, HtgCap ), 1 )
        else
        if( IsWSHPSys > 0 .AND. IfValidAnd( HtgCap > 0 ) )
        then // Calculate scalar for WSHP cooling (heat rejection)
          Min( CoilHtgCapWSHPNew / HtgCap, 1 )
        else 1 // No scaling
        endif endif
      else
      if( Type = "HotWater" .AND. IfValidAnd( HtgCap > 0 ) )
      then
        Min( CoilHtgCapHydronicExisting / ValidOr( Bldg:CoilHtgCapHydronic, HtgCap ), 1 )
      else
      if( IsWSHPSys > 0 .AND. IfValidAnd( HtgCap > 0 ) )
      then // Calculate scalar for WSHP cooling (heat rejection)
        Min( CoilHtgCapWSHPExisting / HtgCap, 1 )
      else 1 // No scaling
      endif endif endif
    else 1
    endif endif
ENDRULE
RULE NEW FluidSys:HtgHlthCareScalar
  DATATYPE
    Float
  LONGFORM
    HeatingHealthCareScalar
  DESCRIPTION
    "The scalar by which basline Healthcare FluidSys heating equipment is scaled."
  INPUTCLASS
