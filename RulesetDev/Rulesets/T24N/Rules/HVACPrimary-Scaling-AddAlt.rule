// -----------------------------------------------------------------------------
// Scale Pump capacity/power
RULE Pump:FlowCapSim
  SIZING_PROPOSED
    if( IfValidAnd( FlowCap > 0 ) .AND.
        FluidSys:IsBaseSys = 0 .AND.
        FluidSys:ScalingReq > 0 )
    then
      if( FluidSys:Type = "ChilledWater" )
      then FlowCap * FluidSys:ClgScalar
      else 
      if( FluidSys:Type = "HotWater" )
      then FlowCap * FluidSys:HtgScalar
      else
      if( IsWSHPSys > 0 )
      then FlowCap * Max( FluidSys:ClgScalar, FluidSys:HtgScalar )
      else
      if( FluidSys:Type = "CondenserWater" )
      then FlowCap * FluidSys:ClgScalar
      else UNCHANGED
      endif endif endif endif
    else UNCHANGED
    endif
  ANNUAL
    if( IfValidAnd( FlowCap > 0 ) .AND.
        FluidSys:ScalingForHlthCareReq > 0 )
    then // Use healthcare scaled value
      if( FluidSys:Type = "ChilledWater" )
      then FlowCap * FluidSys:ClgHlthCareScalar
      else 
      if( FluidSys:Type = "HotWater" )
      then FlowCap * FluidSys:HtgHlthCareScalar
      else
      if( IsWSHPSys > 0 )
      then FlowCap * Max( FluidSys:ClgHlthCareScalar, FluidSys:HtgHlthCareScalar )
      else
      if( FluidSys:Type = "CondenserWater" )
      then FlowCap * FluidSys:ClgHlthCareScalar
      else UNCHANGED
      endif endif endif endif
    else if( IfValidAnd( FlowCap > 0 ) .AND.
        FluidSys:IsBaseSys = 0 .AND.
        FluidSys:ScalingReq > 0 )
    then // Use scaled value
      if( FluidSys:Type = "ChilledWater" )
      then FlowCap * FluidSys:ClgScalar
      else 
      if( FluidSys:Type = "HotWater" )
      then FlowCap * FluidSys:HtgScalar
      else
      if( IsWSHPSys > 0 )
      then FlowCap * Max( FluidSys:ClgScalar, FluidSys:HtgScalar )
      else
      if( FluidSys:Type = "CondenserWater" )
      then FlowCap * FluidSys:ClgScalar
      else UNCHANGED
      endif endif endif endif
    else FlowCap
    endif endif
ENDRULE

RULE Pump:FlowMinSim
  SIZING_PROPOSED
    if( IfValidAnd( FlowMin > 0 ) .AND.
        FluidSys:IsBaseSys = 0 .AND.
        FluidSys:ScalingReq > 0 .AND.
        SpdCtrl = "VariableSpeed" )
    then
      if( FluidSys:Type = "ChilledWater" )
