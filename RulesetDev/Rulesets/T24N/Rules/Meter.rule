RULE CoilClg:ElecMeterRef
  DEFAULT
    if( Proj:Meter > 0 )
    then switch( ParentComponentType() )
          case "ZnSys"    :  ZnSys:ElecMeterRef
          case "AirSys"   :  AirSys:ElecMeterRef
          case "AirSeg"   :  AirSys:ElecMeterRef
          case "TrmlUnit" :  TrmlUnit:ElecMeterRef
          default         :  UNDEFINED
        endswitch
    else UNDEFINED
    endif
  ANNUAL
    if( LocalCompAssigned( ElecMeterRef ) )
    then UNCHANGED
    else if( Proj:Meter > 0 )
         then switch( ParentComponentType() )
               case "ZnSys"    :  ZnSys:ElecMeterRef
               case "AirSys"   :  AirSys:ElecMeterRef
               case "AirSeg"   :  AirSys:ElecMeterRef
               case "TrmlUnit" :  TrmlUnit:ElecMeterRef
               default         :  UNDEFINED
              endswitch
         else UNDEFINED
    endif endif
ENDRULE

