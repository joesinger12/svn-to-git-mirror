RULE VRFSys:ElecMeterRef
  DEFAULT
    if( Proj:Meter > 0 )
    then if( ResArea > 0 .AND. NonResArea = 0 )
         then RuleLibrary( Meter, "ResElecMeter" )
           else if( ResArea = 0 .AND. NonResArea > 0 )
           then RuleLibrary( Meter, "NonResElecMeter" )
           else RuleLibrary( Meter, "MixedElecMeter" )
         endif endif
    else UNDEFINED
    endif  
  ANNUAL
    if( LocalCompAssigned( ElecMeterRef ) )
    then UNCHANGED
    else if( Proj:Meter > 0 )
         then if( ResArea > 0 .AND. NonResArea = 0 )
              then RuleLibrary( Meter, "ResElecMeter" )
              else if( ResArea = 0 .AND. NonResArea > 0 )
              then RuleLibrary( Meter, "NonResElecMeter" )
              else RuleLibrary( Meter, "MixedElecMeter" )
              endif endif
         else UNDEFINED 
    endif endif
ENDRULE

RULE NEW FluidSeg:ResArea
  DESCRIPTION
    "Residential area served by the fluid segment"
  DATATYPE
    Float
  INPUTCLASS
    NotInput
  DEFAULT
    if( FluidSys:Type = "ServiceHotWater" .AND. ( Type = "PrimarySupply" .OR. Type = "SecondarySupply" ) )
    then SumRevRef( Spc:SHWFluidSegRef, Spc:ResArea )
    else if( Type = "PrimarySupply" .OR. Type = "SecondarySupply" )
    then Max( SumRevRef( CoilClg:FluidSegInRef, CoilClg:ResArea ), SumRevRef( CoilHtg:FluidSegInRef, CoilHtg:ResArea ) )
    else UNDEFINED
    endif endif
  ANNUAL
    if( FluidSys:Type = "ServiceHotWater" .AND. ( Type = "PrimarySupply" .OR. Type = "SecondarySupply" ) )
    then SumRevRef( Spc:SHWFluidSegRef, Spc:ResArea )
    else if( Type = "PrimarySupply" .OR. Type = "SecondarySupply" )
    then Max( SumRevRef( CoilClg:FluidSegInRef, CoilClg:ResArea ), SumRevRef( CoilHtg:FluidSegInRef, CoilHtg:ResArea ) )
    else UNDEFINED
    endif endif
ENDRULE

RULE NEW FluidSeg:NonResArea
  DESCRIPTION
    "Non-residential area served by the fluid segment"
  DATATYPE
    Float
  INPUTCLASS
    NotInput
  DEFAULT
    if( FluidSys:Type = "ServiceHotWater" .AND. ( Type = "PrimarySupply" .OR. Type = "SecondarySupply" ) )
    then SumRevRef( Spc:SHWFluidSegRef, Spc:NonResArea )
    else if( Type = "PrimarySupply" .OR. Type = "SecondarySupply" )
    then Max( SumRevRef( CoilClg:FluidSegInRef, CoilClg:NonResArea ), SumRevRef( CoilHtg:FluidSegInRef, CoilHtg:NonResArea ) )
    else UNDEFINED
    endif endif
  ANNUAL
    if( FluidSys:Type = "ServiceHotWater" .AND. ( Type = "PrimarySupply" .OR. Type = "SecondarySupply" ) )
    then SumRevRef( Spc:SHWFluidSegRef, Spc:NonResArea )
    else if( Type = "PrimarySupply" .OR. Type = "SecondarySupply" )
    then Max( SumRevRef( CoilClg:FluidSegInRef, CoilClg:NonResArea ), SumRevRef( CoilHtg:FluidSegInRef, CoilHtg:NonResArea ) )
    else UNDEFINED
    endif endif
ENDRULE

RULE NEW FluidSys:ResArea
  DESCRIPTION
    "Residential area served by the fluid system"
  DATATYPE
    Float
  INPUTCLASS
    NotInput
  DEFAULT
    if( ( SumChildren( FluidSeg:ResArea ) + SumChildren( FluidSeg:NonResArea ) ) = 0 .AND. Type = "CondenserWater" )
    then SumRevRef( FluidSys:CWSysRef, FluidSys:ResArea )
    else SumChildren( FluidSeg:ResArea )
    endif
  ANNUAL
    if( ( SumChildren( FluidSeg:ResArea ) + SumChildren( FluidSeg:NonResArea ) ) = 0 .AND. Type = "CondenserWater" )
    then SumRevRef( FluidSys:CWSysRef, FluidSys:ResArea )
    else SumChildren( FluidSeg:ResArea )
    endif
ENDRULE

RULE NEW FluidSys:NonResArea
  DESCRIPTION
    "Non-residential area served by the fluid system"
  DATATYPE
    Float
  INPUTCLASS
    NotInput
  DEFAULT
    if( ( SumChildren( FluidSeg:ResArea ) + SumChildren( FluidSeg:NonResArea ) ) = 0 .AND. Type = "CondenserWater" )
    then SumRevRef( FluidSys:CWSysRef, FluidSys:NonResArea )
    else SumChildren( FluidSeg:NonResArea )
    endif
  ANNUAL
    if( ( SumChildren( FluidSeg:ResArea ) + SumChildren( FluidSeg:NonResArea ) ) = 0 .AND. Type = "CondenserWater" )
    then SumRevRef( FluidSys:CWSysRef, FluidSys:NonResArea )
    else SumChildren( FluidSeg:NonResArea )
    endif
ENDRULE

RULE FluidSys:ElecMeterRef
  DEFAULT
    if( Proj:Meter > 0 )
    then if( ResArea > 0 .AND. NonResArea = 0 )
         then RuleLibrary( Meter, "ResElecMeter" )
         else if( ResArea = 0 .AND. NonResArea > 0 )
         then RuleLibrary( Meter, "NonResElecMeter" )    
         else RuleLibrary( Meter, "MixedElecMeter" )
         endif endif
    else UNDEFINED
    endif
  ANNUAL
    if( Proj:Meter > 0 )
    then if( LocalCompAssigned( ElecMeterRef ) )
         then UNCHANGED
         else if( ResArea > 0 .AND. NonResArea = 0 )
         then RuleLibrary( Meter, "ResElecMeter" )
         else if( ResArea = 0 .AND. NonResArea > 0 )
         then RuleLibrary( Meter, "NonResElecMeter" )    
         else RuleLibrary( Meter, "MixedElecMeter" )
         endif endif endif
    else UNDEFINED
    endif
