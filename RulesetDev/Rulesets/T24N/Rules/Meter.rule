RULE Proj:Meter
  DESCRIPTION
    "Flag that turns on EnergyPlus meter report, which will be written to a CSV file"
  DEFAULT
    0
ENDRULE

// --------------- Defaulting MeterRef ---------------------------------
RULE NEW Spc:ResArea
  DESCRIPTION
    "residential area"
  DATATYPE
    Float
  INPUTCLASS
    NotInput
  DEFAULT
    if( IsRes > 0 .OR. IfValidAnd( IsResCommon > 0 ))
    then CondFlrAreaWithMult
    else 0
    endif
ENDRULE

RULE NEW Spc:NonResArea
  DESCRIPTION
    "residential area"
  DATATYPE
    Float
  INPUTCLASS
    NotInput
  DEFAULT
    if( ResArea = 0 )
    then CondFlrAreaWithMult
    else 0
    endif
ENDRULE

RULE Spc:ElecMeterRef
  DEFAULT
    if( Proj:Meter > 0 )
    then if( ResArea )
         then RuleLibrary( Meter, "ResElecMeter" )
         else RuleLibrary( Meter, "NonResElecMeter" )
         endif
    else UNDEFINED
    endif
ENDRULE

RULE Spc:GasMeterRef
  DEFAULT
    if( Proj:Meter > 0 )
    then if( ResArea )
         then RuleLibrary( Meter, "ResGasMeter" )
         else RuleLibrary( Meter, "NonResGasMeter" )
         endif
    else UNDEFINED
    endif
ENDRULE

