RULE NEW ResIAQFan:IAQCFMExh
  DATATYPE
    Float
  LONGFORM
    IAQCFMExhaust
  INPUTCLASS
    NotInput
  UNITS
    cfm
  DEFAULT
    if(IAQFanType = "Exhaust") then
      IAQCFM
    else
      0
    endif
ENDRULE

RULE NEW ResIAQFan:WperCFMExh
  DATATYPE
    Float
  LONGFORM
    WattsPerCFMExhaust
  INPUTCLASS
    NotInput
  UNITS
    W/cfm
  DEFAULT
    if(IAQFanType = "Exhaust") then
      WperCFMIAQ
    else
      0
    endif
ENDRULE

