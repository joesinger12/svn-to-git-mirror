RULE NEW ResIAQFan:IAQCFMBal
  DATATYPE
    Float
  LONGFORM
    IAQCFMBalanced
  INPUTCLASS
    NotInput
  UNITS
    cfm
  DEFAULT
    if(IAQFanType = "Balanced" .AND.
       ((IfValidAnd(Proj:AnalysisVersion >= 190200)==0 .AND. IfValidAnd(IAQRecovEffect = 0)) .OR.
        (IfValidAnd(Proj:AnalysisVersion >= 190200)    .AND. IfValidAnd(IncludesRecov  = 0)))) then
      IAQCFM
    else
      0
    endif
ENDRULE

RULE NEW ResIAQFan:WperCFMBal
  DATATYPE
    Float
  LONGFORM
    WattsPerCFMBalanced
  INPUTCLASS
    NotInput
  UNITS
    W/cfm
  DEFAULT
    if(IAQFanType = "Balanced" .AND.
       ((IfValidAnd(Proj:AnalysisVersion >= 190200)==0 .AND. IfValidAnd(IAQRecovEffect = 0)) .OR.
        (IfValidAnd(Proj:AnalysisVersion >= 190200)    .AND. IfValidAnd(IncludesRecov  = 0)))) then
      WperCFMIAQ
    else
      0
    endif
ENDRULE

RULE NEW ResIAQFan:IAQHRVCFMxRcvEff
  DATATYPE
    Float
  LONGFORM
    IAQHRVCFMxRcvEff
  INPUTCLASS
    NotInput
  UNITS
    CFM-%
  DEFAULT
    if(IAQFanType = "Balanced") then
      IAQCFM * ValidOr(IAQRecovEffect, 0)
    else
      0
    endif
ENDRULE

RULE NEW ResIAQFan:IAQHRVCFMxSRE
  DATATYPE
    Float
  LONGFORM
    IAQHRVCFMxSRE
  INPUTCLASS
    NotInput
  UNITS
    CFM-%
  DEFAULT
    if(IAQFanType = "Balanced" .AND. IncludesRecov > 0) then
      IAQCFM * ValidOr(SensRecovEff, 0)
    else
      0
    endif
ENDRULE

RULE NEW ResIAQFan:IAQHRVCFMxASRE
  DATATYPE
    Float
  LONGFORM
    IAQHRVCFMxASRE
  INPUTCLASS
    NotInput
  UNITS
    CFM-%
  DEFAULT
    if(IAQFanType = "Balanced" .AND. IncludesRecov > 0) then
      IAQCFM * ValidOr( AdjSensRecovEff, 0 )
    else
      0
    endif
ENDRULE


