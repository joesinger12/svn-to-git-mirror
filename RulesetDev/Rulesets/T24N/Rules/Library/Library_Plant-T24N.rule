  PumpRef = "Base CW Pump"
  ..
FluidSegment   "BaseCWPriSupSeg"
  Type = "PrimarySupply"
  ..
FluidSegment   "BaseCWPriRetSeg"
  Type = "PrimaryReturn"
  ..
Pump   "Base CW Pump"                       
  ..

