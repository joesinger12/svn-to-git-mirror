// ********** ProcLd Object For Load Passing ************************************
ProcLd  "ResLoad"
  LdSchRef = "ResLoadSch"
  PkFlowRt = 15850
  FlowSchRef = "ResFlowSch"
 ..
  
Sch "ResLoadSch"
  Type = "File"
  SchFileName = "RSYS-RESZONELOADS.csv"
  ColIdx = 8
  RowsToSkipAtTop = 4
  ColSeparator = "Comma"
  InterpToTimestep = "Yes"
  MinutesPerRow = 60
 ..

Sch "ResFlowSch"
  Type = "File"
  SchFileName = "RSYS-RESZONELOADS.csv"
  ColIdx = 7
  RowsToSkipAtTop = 4
  ColSeparator = "Comma"
  InterpToTimestep = "Yes"
  MinutesPerRow = 60
 ..
 
