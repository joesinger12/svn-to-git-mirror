    if ( Spc:IntLtgSpecMthd = "CompleteBuildingMethod" ) 
      then 0
    else if ( Spc:IntLtgSpecMthd = "AreaCategoryMethod" ) 
      then ACMTotAllowPwr
    else if ( Spc:IntLtgSpecMthd = "TailoredMethod" ) 
      then TMTotAllowPwr
    else 0
