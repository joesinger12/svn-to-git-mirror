  DATATYPE
    cseZONE
  INPUTCLASS
    NotInput
  DEFAULT
    UNDEFINED
ENDRULE


// ----------------------------------------------------------------------------- SAC 02/28/22
RULE NEW Proj:AvgFlrHgt
  LONGFORM
    AverageFloorHeight
  DATATYPE
    Float
  DESCRIPTION
    ""
  INPUTCLASS
    NotInput
  DEFAULT
    if (IfValidAnd( Bldg:TotStoryCnt > 0 )==0) then  9
    else  if (ComponentCount( Spc ) > 0 .AND. ComponentCount( ResZn ) > 0 .AND. ComponentCount( ResOtherZn ) > 0)
          then  ( max(  max(  MaxAll( Spc:MaxZ ),  MaxAll( ResZn:Top    )  ),  MaxAll( ResOtherZn:Top    )  ) -
                  min(  min(  MinAll( Spc:FlrZ ),  MinAll( ResZn:Bottom )  ),  MinAll( ResOtherZn:Bottom )  ) ) / Bldg:TotStoryCnt
          else if (ComponentCount( Spc ) > 0 .AND. ComponentCount( ResZn ) > 0 )
          then  ( max(  MaxAll( Spc:MaxZ ),  MaxAll( ResZn:Top    )  ) -
                  min(  MinAll( Spc:FlrZ ),  MinAll( ResZn:Bottom )  ) ) / Bldg:TotStoryCnt
          else if (ComponentCount( Spc ) > 0 .AND. ComponentCount( ResOtherZn ) > 0)
          then  ( max(  MaxAll( Spc:MaxZ ),  MaxAll( ResOtherZn:Top    )  ) -
                  min(  MinAll( Spc:FlrZ ),  MinAll( ResOtherZn:Bottom )  ) ) / Bldg:TotStoryCnt
          else if (ComponentCount( ResZn ) > 0 .AND. ComponentCount( ResOtherZn ) > 0)
          then  ( max(  MaxAll( ResZn:Top    ),  MaxAll( ResOtherZn:Top    )  ) -
                  min(  MinAll( ResZn:Bottom ),  MinAll( ResOtherZn:Bottom )  ) ) / Bldg:TotStoryCnt
          else if (ComponentCount( Spc ) > 0)
          then        ( MaxAll( Spc:MaxZ ) - MinAll( Spc:FlrZ ) ) / Bldg:TotStoryCnt
          else if (ComponentCount( ResZn ) > 0)
          then        ( MaxAll( ResZn:Top ) - MinAll( ResZn:Bottom ) ) / Bldg:TotStoryCnt
          else if (ComponentCount( ResOtherZn ) > 0)
          then        ( MaxAll( ResOtherZn:Top ) - MinAll( ResOtherZn:Bottom ) ) / Bldg:TotStoryCnt
          else  9
          endif endif endif endif endif endif endif
    endif
ENDRULE


