//  This rule file addresses the following building descriptors:

// Process areas served by system
// Computer room
RULE NEW AirSys:CompRmArea
  DATATYPE
    Float
  LONGFORM
    ComputerRoomArea
  INPUTCLASS 
    NotInput
  DEFAULT
    Max( SumRevRef( ThrmlZn:PriAirCondgSysRef[1], ThrmlZn:CompRmArea ),
         SumRevRef( ThrmlZn:VentSysRef, ThrmlZn:CompRmArea ) )
  SIZING
    Max( SumRevRef( ThrmlZn:PriAirCondgSysRef[1], ThrmlZn:CompRmArea ),
         SumRevRef( ThrmlZn:VentSysRef, ThrmlZn:CompRmArea ) )
ENDRULE
RULE NEW ZnSys:CompRmArea
  DATATYPE
    Float
  LONGFORM
    ComputerRoomArea
  INPUTCLASS 
    NotInput
  DEFAULT
