              SumRevRef( ThrmlZn:PriAirCondgSysRef[2], ThrmlZn:CompRmArea ) ),
         SumRevRef( ThrmlZn:VentSysRef, ThrmlZn:CompRmArea ) )
  SIZING
    Max( Max( SumRevRef( ThrmlZn:PriAirCondgSysRef[1], ThrmlZn:CompRmArea ),
              SumRevRef( ThrmlZn:PriAirCondgSysRef[2], ThrmlZn:CompRmArea ) ),
         SumRevRef( ThrmlZn:VentSysRef, ThrmlZn:CompRmArea ) )
ENDRULE
